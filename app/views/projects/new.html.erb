<div class="container">
  <div class="box col-10">

    <h1>Start a new project</h1>

    <%= simple_form_for @project, url: projects_path do |f|%>
      <%= f.input :title %>
      <br/>
      <%= f.input :description %>
      <%= f.submit 'Create', class: 'btn btn-primary make-it-green' %>
    <% end %>

  </div>
</div>
